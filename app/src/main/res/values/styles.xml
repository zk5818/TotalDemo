<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item></style>

    <style name="TestPop">
        <item name="android:windowEnterAnimation">@anim/pop_in</item>
        <item name="android:windowExitAnimation">@anim/pop_out</item>
    </style>

    <style name="Color1SwitchStyle">
        <item name="colorControlActivated">@color/c_6DC8D3</item>
        <!-- 关闭时的拇指按钮的颜色 -->
        <item name="colorSwitchThumbNormal">@color/colorPrimary</item>
        <!-- witch关闭时的轨迹的颜色  30%这个颜色 -->
        <item name="android:colorForeground">@color/c_F64B72</item>
    </style>
</resources>
